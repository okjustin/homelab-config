[
  {
    "id": "homelab-deploy",
    "execute-command": "/deploy.sh",
    "command-working-directory": "/repos/homelab-config",
    "trigger-rule": {
      "match": {
        "type": "value",
        "value": "__WEBHOOK_SECRET__",
        "parameter": {
          "source": "header",
          "name": "X-Hub-Signature-256"
        }
      }
    }
  }
]